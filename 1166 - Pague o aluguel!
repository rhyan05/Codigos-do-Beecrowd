divida = int(input())
pagamentomensal = int(input())
contador = 0

while divida != 0:
    contador += 1

    if divida >= pagamentomensal:
        antes = divida
        divida = divida - pagamentomensal
        depois = divida
    else:
        antes = divida
        divida = divida - divida
        depois = divida

    print(f'pagamento: {contador}')
    print(f'antes = {antes}')
    print(f'depois = {depois}')
    print('-----')
